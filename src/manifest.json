{
  "manifest_version": 2,
  "name": "Count My Vultr",
  "version": "0.1",
  "browser_action": {
   "default_icon": "img.png"
  },
  "content_scripts": [
	 {
	    //Set your address you want the extension will work in mataches!!! 
	 "matches": ["https://my.vultr.com/*", "http://my.vultr.com/*"],
	  "js": ["jquery-3.2.1.min.js","content.js"],
	  "run_at": "document_start"
	 }
  ],
  "icons": { "128": "img.png" }
}

